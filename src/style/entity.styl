@import './lib';

.entityPrint
  font-family $workssans
  overflow hidden
  text-overflow ellipsis

.entityName
  width 230px
  position relative
  white-space nowrap
  font-style normal
  font-weight 400
  font-size 16px
  line-height 19px
  color #FFFFFF

[data-entity-name-type="class"]
  color $green

[data-entity-name-type="mixin"]
  color $orange
