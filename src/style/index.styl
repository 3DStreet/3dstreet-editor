@import './lib';

body.aframe-inspector-opened,
.toggle-edit
  font-family $mainFont

.wf-roboto-n4-active body.aframe-inspector-opened,
.wf-roboto-n4-active .toggle-edit
  font-family $mainFont

body.aframe-inspector-opened
  background $bgdark
  color #fff
  font-size 12px
  margin 0
  overflow hidden

#aframeInspector
  @import './scenegraph';
  @import './components';
  @import './entity';
  @import './select';
  @import './textureModal';
  @import './viewport';
  @import './widgets';

  body
    font-family $mainFont !important

  .Select,
  code,
  pre,
  input,
  textarea,
  select,
  button,
  a,
  p,
  h1,
  h2,
  h3,
  h4,
  nav,
  ul,
  ol,
  li,
  main,
  header,
  footer,
  aside
    font-family $mainFont
    font-size 13px

  .wf-robotomono-n4-active .Select,
  .wf-robotomono-n4-active code,
  .wf-robotomono-n4-active pre,
  .wf-robotomono-n4-active input,
  .wf-robotomono-n4-active textarea,
  .wf-robotomono-n4-active select
    font-family mainFont

  hr
    border 0
    border-top 1px solid #ccc

  a
    cursor pointer

  button
    position relative

  code
    font-family Consolas, Andale Mono, Monaco, Lucida Console, Liberation Mono, DejaVu Sans Mono, Bitstream Vera Sans Mono, Courier New, monospace

  textarea
    tab-size 4
    white-space pre
    word-wrap normal

  textarea.success
    border-color #8b8 !important

  textarea.fail
    background-color rgba(255, 0, 0, 0.05)
    border-color #f00 !important

  textarea,
  input
    outline none /* osx */

  .viewButton
    box-sizing border-box
    font-weight 500
    font-size 16px
    width 70px
    height 43px
    padding 12px 16px
    background $button
    border-radius 18px

  .viewButton:hover
    background $buttonHover

  .viewButton:active
    background-color $buttonPressed
    color $colorPressed

  .gltfIcon,
  .screenshotIcon
    height 43px 
    width 43px

  .screenshotIcon div,
  .gltfIcon div
    background-repeat no-repeat
    background-size inherit
    background-position center
    display block
    height 43px  
    position relative
    width 43px

  .screenshotIcon div  
    background-image url("data:image/svg+xml;base64,")

  .screenshotIcon:hover div 
    background-image url("data:image/svg+xml;base64,") 

  .screenshotIcon:active div 
    background-image url("data:image/svg+xml;base64,") 

  .gltfIcon div  
    background-image url("data:image/svg+xml;base64,")  

  .gltfIcon:active div 
    background-image url("data:image/svg+xml;base64,")  
    background-color #5B37C0
    border-radius 22px

  #scenegraph,
  #rightPanel
    z-index 9998

  #sidebar,
  #scenegraph,
    .panel
      cursor default
      user-select none

  .toggle-edit
    background-image url("data:image/svg+xml;base64,")
    background-repeat no-repeat
    background-size contain
    background-position center
    left 40px
    position fixed
    text-align center
    text-decoration none
    top 42px
    width 221px
    height 36px
    z-index 999999999

  .logo-editor 
    background-image: url('../../assets/3DStreet-Editor.svg');

  .logo-viewer
    background-image: url('../../assets/3DStreet-Viewer.svg')

  .toggle-edit .logo-img
    position relative
    left 245px
    top -3.5px
    display flex
    width fit-content
    white-space nowrap
    text-align center
    box-sizing border-box
    font-size 16px
    line-height 19.2px
    color #FFF
    letter-spacing 0.1em
    background-color #8941FF
    border-radius 18px
    padding 12px 16px
    transition all 0.3s

  .toggle-edit .logo-img:hover
    background-color #6439df
    transition all 0.3s

  input
    background-color transparent
    border 1px solid #555
    color #fff

  input,
  .texture canvas
    transition 0.1s background-color ease-in-out, 0.1s border-color ease-in-out, 0.1s color ease-in-out

  input[type=text],
  input[type=number],
  input.string,
  input.number
    min-height 14px
    outline none

  input[type="checkbox"]
    visibility hidden         

  input.number
    background-color transparent
    border 0
    color #2cb7ff
    cursor col-resize
    font-size 13px
    padding 2px

  input.stringfocus,
  input.numberfocus
    border 1px solid #20b1fb
    color #fff
    cursor auto

  input.error
    border 1px solid #a00

  #sidebar
    justify-content flex-start
    box-sizing border-box
    color #c3c3c3
    cursor default
    flex 1 1 auto
    font-size 16px
    outline none
    overflow-y auto
    overflow-x hidden
    position absolute
    right 0px
    top 80px
    padding-bottom 32px
    padding-left 16px
    width 380px
    height 584px
    display flex
    flex-direction column
    align-items flex-start
    background #373737
    border-radius 12px 0px 0px 12px

    #entity-name
      width 364px
      display flex
      flex-direction row
      justify-content space-between
      align-items center

      span
        background-image url("data:image/svg+xml;base64,")
        width 252px
        font-weight 500
        font-size 24px
        white-space nowrap
        overflow hidden
        text-overflow ellipsis
        color #fff
        height 28px
        line-height 28px
        background-repeat no-repeat
        background-position left
        padding-left 36px
        margin-left 8px

      #toggle-rightbar
        width 56px
        height 72px
        background #323232 
        background-image url("data:image/svg+xml;base64,")
        background-repeat no-repeat
        background-position center 
        cursor pointer 

      #toggle-rightbar:hover
        background-color $buttonHover 

      #toggle-rightbar:active
        background-color $buttonPressed

  #sidebar.hide,
  #sidebar.hide #entity-name
    padding 0px
    width 56px
    height 72px
    display block
    overflow hidden

  #sidebar.hide:hover
    width auto
    max-width 380px

    .components
      display none

    #entity-name
      width auto
      background-color $buttonHover
      cursor pointer
      padding-right 16px

      span
        font-size 18px
        font-weight 500
        width auto
        display block !important
        margin-top 22px
        background-image url($iconProps)
        background-position right
        padding-left 4px
        padding-right 36px
        max-width 336px

      #toggle-rightbar
        display none

  #sidebar.hide span  
    display none

  #sidebar.hide:active #entity-name
    background-color $buttonPressed

    span      
      color $colorPressed !important
      background-image url($iconPropsActive)

  #sidebar.hide #toggle-rightbar
    background rgba(50, 50, 50, 0.5) !important
    backdrop-filter blur(24px)
    background-image url($iconProps) !important
    background-repeat no-repeat !important
    background-position center !important

  #sidebar.hide #toggle-rightbar:hover  
    background-color $buttonHover !important

  #sidebar.hide #toggle-rightbar:active
    background-color $buttonPressed !important

  #sidebar *
    vertical-align middle

  input,
  textarea,
  select
    background $black
    border 1px solid transparent
    color #888

  select
    background $bglighter

  input[type=color]
    background-color #333
    border 1px solid #111
    height 28px
    cursor pointer

  input[type=color]
    cursor pointer
    height 25px
    padding 0
    width 50px

  /* Note these vendor-prefixed selectors cannot be grouped! */
  input[type=color]-webkit-color-swatch
    border 0  /* To remove the gray border. */

  input[type=color]-webkit-color-swatch-wrapper
    padding 0  /* To remove the inner padding. */

  input[type=color]-moz-color-swatch
    border 0

  input[type=color]-moz-focus-inner
    border 0  /* To remove the inner border (specific to Firefox). */
    padding 0

  .hidden
    visibility hidden

  a.button
    color #bcbcbc
    font-size 16px
    margin-left 10px
    text-decoration none

  a.buttonhover
    color #1faaf2

  @keyframes animateopacity
    from { opacity: 0 }
    to { opacity: 1 }

  .hide
    display none

  .a-canvas.state-dragging
    cursor grabbing

  #rightPanel
    align-items stretch
    display flex
    justify-content flex-end

  #inspectorContainer
    display flex
    justify-content space-between
    left 0
    height 100%
    pointer-events none
    position fixed
    top 36px
    width 100%
    z-index 999999

  #scenegraph,
  #viewportBar,
  #rightPanel
    pointer-events all

  .aframe-inspector-opened a-scene .a-canvas
    background-color #191919
    z-index 9998

  .toggle-sidebar
    align-items center
    display flex
    height 100%
    position absolute
    z-index 9998

    .left
      left 0

    .right
      right 0

    a
      background-color #262626
      color #bcbcbc
      padding 5px
      z-index 9998

    a.hover
      background-color #1faaf2
      color #fff
