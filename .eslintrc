{
  "extends": ["eslint:recommended", "plugin:react/recommended", "standard"],
  "rules": {
    "semi": [2, "always"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "AFRAME": true,
    "THREE": true
  },
  "parser": "babel-eslint",
  "parserOptions": {,
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "classes": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
     }
  },
  "plugins": [
    "react"
  ]
}
